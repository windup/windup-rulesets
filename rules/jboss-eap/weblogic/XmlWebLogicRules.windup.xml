<?xml version="1.0"?>
<ruleset xmlns="http://windup.jboss.org/schema/jboss-ruleset" id="XmlWebLogicRules"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://windup.jboss.org/schema/jboss-ruleset http://windup.jboss.org/schema/jboss-ruleset/windup-jboss-ruleset.xsd">
    <rules>
        <rule id="XmlWebLogicRules_2fmb">
            <when>
                <xmlfile matches="/*[local-name()='weblogic-rdbms-jar']"/>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic Entity EJB Configuration" effort="3"/>
                    <xslt title="JBoss EJB CMP Descriptor (Windup-Generated)" template="transformations/xslt/weblogic-entity2-to-jboss.xsl" extension="-jbosscmp-jdbc.xml"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_4mthy">
            <when>
                <xmlfile matches="//*[local-name()='delay-updates-until-end-of-tx']/text()"/>
            </when>
            <perform>
                <iteration>
                    <hint effort="0"><message>WebLogic Specific Transaction Property Delay Updates</message>
                    </hint>
                    <hint effort="1"><message>In EAP6 replace with: &amp;lt;sync-on-commit-only&amp;gt; in jbosscmp-jdbc.xml</message>
                    </hint>
                </iteration>
            </perform>
        </rule>

        <rule id="XmlWebLogicRules_6bcvk">
            <when>
                <xmlfile as="default" matches="/weblogic-ejb-jar">
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic EJB XML" effort="3"/>
                    <xslt title="JBoss EJB Descriptor (Windup-Generated)" template="transformations/xslt/weblogic-ejb-to-jboss.xsl" extension="-jboss.xml"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_8jyqn">
            <when>
                <xmlfile as="default" matches="/wl9:weblogic-ejb-jar">
                    <namespace prefix="wl9" uri="http://www.bea.com/ns/weblogic/90"/>
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic EJB XML" effort="3"/>
                    <xslt title="JBoss EJB Descriptor (Windup-Generated)" template="transformations/xslt/weblogic9-ejb-to-jboss.xsl" extension="-jboss.xml"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_Altni">
            <when>
                <xmlfile as="default" matches="/wl10:weblogic-ejb-jar">
                    <namespace prefix="wl10" uri="http://www.bea.com/ns/weblogic/10.0"/>
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic EJB XML" effort="3"/>
                    <xslt title="JBoss EJB Descriptor (Windup-Generated)" template="transformations/xslt/weblogic10-ejb-to-jboss.xsl" extension="-jboss.xml"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_Cbxnb">
            <when>
                <xmlfile as="default" matches="/wl10:weblogic-wsee-standaloneclient">
                    <namespace prefix="wl10" uri="http://www.bea.com/ns/weblogic/weblogic-wsee-standaloneclient"/>
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic SOAP Client Mapping" effort="6"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_Epnmk">
            <when>
                <xmlfile as="default" matches="/jee:java-wsdl-mapping">
                    <namespace prefix="jee" uri="http://java.sun.com/xml/ns/j2ee"/>
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="Java to WSDL Mapping" effort="3"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_10vvyf">
            <when>
                <xmlfile as="default" matches="/*[local-name()='weblogic-application']">
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic EAR Application Descriptor" effort="3"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_12idl">
            <when>
                <xmlfile as="default" matches="/wl:webservice-policy-ref | /wl9:webservice-policy-ref">
                    <namespace prefix="wl9" uri="http://www.bea.com/ns/weblogic/90"/>
                    <namespace prefix="wl" uri="http://www.bea.com/ns/weblogic/webservice-policy-ref"/>
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic Webservice Policy" effort="0"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_14wscy">
            <when>
                <xmlfile as="webservices" matches="/wl:weblogic-webservices | /wl9:weblogic-webservices">
                    <namespace prefix="wl9" uri="http://www.bea.com/ns/weblogic/90"/>
                    <namespace prefix="wl" uri="http://www.bea.com/ns/weblogic/weblogic-webservices"/>
                </xmlfile>
                <xmlfile as="webservices-types" matches="//wl:webservice-type | //wl9:webservice-type" from="webservices">
                    <namespace prefix="wl9" uri="http://www.bea.com/ns/weblogic/90"/>
                    <namespace prefix="wl" uri="http://www.bea.com/ns/weblogic/weblogic-webservices"/>
                </xmlfile>
            </when>
            <perform>
                <iteration over="webservices">
                    <classification title="WebLogic Webservice Descriptor" effort="0"/>
                </iteration>
                <iteration over="webservices-types">
                    <hint effort="0"><message>Webservice Type</message>
                    </hint>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_16aynk">
            <when>
                <xmlfile as="default" matches="/*[local-name()='weblogic-jms']">
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic JMS Descriptor" effort="1"/>
                    <xslt title="JBoss Messaging Queue/Topic Configuration (Windup-Generated)" template="transformations/xslt/weblogic-jms-to-jboss-messaging.xsl" extension="-jms-queuetopic-service.xml"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_18bicn">
            <when>
                <xmlfile as="default" matches="/bea:weblogic-web-app | /wlso:weblogic-web-app | /wls:weblogic-web-app | /weblogic-web-app">
                    <namespace prefix="wlso" uri="http://xmlns.oracle.com/weblogic/weblogic-web-app"/>
                    <namespace prefix="bea" uri="http://www.bea.com/ns/weblogic/90"/>
                    <namespace prefix="wls" uri="http://www.bea.com/ns/weblogic/weblogic-web-app"/>
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic Web Application Descriptor" effort="3"/>
                    <xslt title="JBoss Web EAP5 (Windup-Generated)" template="transformations/xslt/weblogic-jboss5-web-xml.xsl" extension="-jboss-web.xml"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_1Agqai">
            <when>
                <xmlfile as="default" public-id="BEA.+RMI Runtime DTD 1..">
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic RMI XML Version 1.x" effort="0"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_1Csnki">
            <when>
                <xmlfile as="default" matches="/wsdd:type-mapping">
                    <namespace prefix="wsdd" uri="http://www.bea.com/servers/wls70"/>
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic Webservice Type Mapping" effort="0"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_1Efbtf">
            <when>
                <xmlfile as="default" matches="/wlw:wlw-config">
                    <namespace prefix="wlw" uri="http://www.bea.com/2003/03/wlw/config/"/>
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic Services Configuration" effort="0"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_20xslg">
            <when>
                <xmlfile as="default" matches="/project/taskdef[@classname='weblogic.ant.taskdefs.webservices.servicegen.ServiceGenTask'] | /project/taskdef[@classname='weblogic.ant.taskdefs.webservices.javaschema.JavaSchema'] | /project/taskdef[@classname='weblogic.ant.taskdefs.webservices.autotype.JavaSource2DD'] | /project/taskdef[@classname='weblogic.ant.taskdefs.webservices.clientgen.ClientGenTask']">
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic-specifc Webservice Ant Tasks" effort="10"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlWebLogicRules_22ablx">
            <when>
                <xmlfile as="default" matches="/wl:annotation-manifest | /annotation-manifest">
                    <namespace prefix="wl" uri="http://www.bea.com/2004/03/wlw/external-config/"/>
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification title="WebLogic Annotation Manifest" effort="8"/>
                </iteration>
            </perform>
        </rule>
    </rules>
</ruleset>
