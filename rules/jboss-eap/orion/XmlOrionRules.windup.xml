<?xml version="1.0"?>
<ruleset xmlns="http://windup.jboss.org/ns/ruleset/2.3.0.Final"  id="XmlOrionRules"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"     xsi:schemaLocation="http://windup.jboss.org/ns/ruleset/2.3.0.Final http://windup.jboss.org/schema/windup-ruleset-2.3.0.Final.xsd"> 
    <rules>
        <rule id="XmlOrionRules_2fmb">
            <when>
                <xmlfile as="default" matches="/orion-ejb-jar">
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification classification="Oracle Application Platform EJB Descriptor" effort="3"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlOrionRules_4mthy">
            <when>
                <xmlfile as="default" matches="/orion-web-app">
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification classification="Oracle Application Platform Web Descriptor " effort="3"/>
                </iteration>
            </perform>
        </rule>
        <rule id="XmlOrionRules_6bcvk">
            <when>
                <xmlfile as="default" matches="/orion-application">
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification classification="Oracle Application Platform EAR Descriptor " effort="3"/>
                </iteration>
            </perform>
        </rule>
    </rules>
</ruleset>
