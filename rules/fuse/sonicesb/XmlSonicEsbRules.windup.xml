            <when>
                <xmlfile as="default" matches="/xq:serviceType">
                    <namespace prefix="xq" uri="http://www.sonicsw.com/sonicxq"/>
                </xmlfile>
            </when>
            <perform>
                <iteration>
                    <classification classification="SonicESB Service Configuration" effort="4"/>
                </iteration>
            </perform>
        </rule>
    </rules>
</ruleset>
