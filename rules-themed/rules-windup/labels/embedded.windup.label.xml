<?xml version="1.0" encoding="UTF-8"?>
<labelset id="embedded_labels"
          priority="1"
          xmlns="http://windup.jboss.org/schema/jboss-labelset"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://windup.jboss.org/schema/jboss-labelset http://windup.jboss.org/schema/jboss-labelset/windup-jboss-labelset.xsd">
    <labels>
        <label id="springboot">
            <name>SpringBoot</name>
            <description>Spring Boot Application</description>
            <supported>
                <tag>Bean Validation</tag>
                <tag>JAXB</tag>
                <tag>JSF Page</tag>
                <tag>JSP Page</tag>
                <tag>Servlet</tag>
                <tag>Application Properties File</tag>
                <tag>Spring Cloud Config</tag>
                <tag>Spring Properties</tag>
                <!-- Sprign-JPA-->
                <tag>JPA</tag> <!--based on Jarkata annotation {ee-flavor}.persistence-->
                <tag>JPA entities</tag><!--based on Jarkata annotation {ee-flavor}.persistence-->
                <tag>JPA named queries</tag><!--based on Jarkata annotation {ee-flavor}.persistence-->
                <tag>Spring Transactions</tag>
                <tag>Common Annotations</tag> <!-- support by Spring -->
                <tag>JavaMail</tag> <!--no model found but no dependency on container  -->
                <tag>JAX-RS</tag> <!--based on Jarkata annotation {ee-flavor}.ws.rs.Path-->
                <tag>JAX-WS</tag> <!--based on Jarkata annotation {ee-flavor}.jws.WebService-->
                <tag>CDI</tag> <!--3rd party support-->
                <tag>Spring Boot Application</tag> <!--annotation with org.springframework.boot.autoconfigure.SpringBootApplication-->
                <tag>Spring</tag>  <!--spring*.jar-->
                <tag>Spring Batch</tag> <!--spring-batch{*}.jar-->
                <tag>Spring Boot</tag> <!--spring-boot{*}.jar-->
                <tag>Spring Boot Actuator</tag> <!-- dependency of spring-boot-actuator -->
                <tag>Spring Boot Flo</tag> <!-- file {prefixes}flo-editor -->
                <tag>Spring Cloud Function</tag> <!-- dependency spring-cloud-function-context -->
                <tag>Spring Data</tag> <!-- spring-data{*}.jar -->
                <tag>Spring Data JPA</tag> <!-- dependency of spring-data-jpa -->
                <tag>Spring DI</tag>  <!--dependency of spring-beans-->
                <tag>Spring Integration</tag> <!--integration jar, IntegrationFlow in class and channel in xml files-->
                <tag>Spring JMX</tag> <!--jmx class and jmx exporter in xml -->
                <tag>Spring Messaging Client</tag> <!--spring-jms and spring-message -->
                <tag>Spring MVC</tag> <!-- spring-webmvc jar-->
                <tag>Spring Scheduled</tag> <!--references="org.springframework.scheduling.annotation.Scheduled"-->
                <tag>Spring Security</tag>
                <tag>Spring Test</tag>
                <tag>Spring XML</tag>                
            </supported>
            <unsuitable>
                <tag>Clustering EJB</tag>
                <tag>Clustering Web Session</tag>
                <tag>EAR</tag>
                <tag>EJB</tag>
                <tag>EJB XML*</tag>
                <tag>Enterprise Web Services</tag>
                <tag>JACC</tag>
                <tag>Java EE</tag>
                <tag>Java EE Batch</tag>
                <tag>Java EE Batch API</tag>
                <tag>Java EE JSON-P</tag>
                <tag>Java EE Security</tag>
                <tag>JBoss EJB XML</tag>
                <tag>JBoss Web XML</tag>
                <tag>JCA</tag>
                <tag>JMS</tag> <!--no resource found-->
                <tag>JMS Connection Factory</tag> <!--base on JNDI-->
                <tag>JMS Queue</tag> <!--base on JNDI-->
                <tag>JMS Topic</tag> <!--base on JNDI-->
                <tag>JTA</tag> <!--based on JNDI data source -->
                <tag>MEJB</tag>
                <tag>Message (MDB)</tag>
                <tag>RMI</tag>
                <tag>SOAP (SAAJ)</tag>
