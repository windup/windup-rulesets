                <tag>WebSphere WS Extension</tag>
                <tag>WS Metadata</tag>
                <tag>Applet</tag>
                <tag>JNLP</tag>
                <tag>Eclipse RCP</tag>                
            </unsuitable>
            <neutral>
                <tag>Java Source</tag>
                <tag>JDBC datasources</tag>
                <tag>Manifest</tag>
                <tag>Maven XML</tag>
                <tag>Properties</tag>
                <tag>Web XML*</tag>
