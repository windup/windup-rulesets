        <rule id="technology-usage-mvc-01000">
            <when>
                <graph-query discriminator="TechnologyTagModel">
                    <property name="name">Apache Wicket (embedded)</property>
                </graph-query>
            </when>
            <perform>
                <technology-identified name="Wicket">
                    <tag name="View"/>
                    <tag name="Embedded"/>
                    <tag name="MVC"/>
                </technology-identified>
            </perform>
        </rule>
