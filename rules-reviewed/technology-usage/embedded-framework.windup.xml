                </classification>
                <technology-tag level="INFORMATIONAL">Spring Shell</technology-tag>
            </perform>
        </rule>
        <rule id="embedded-framework-08600">
            <when>
                <filecontent pattern="{prefixes}flo-editor" filename="{*}.{extensions}" />
            </when>
            <perform>
                <classification title="Embedded framework - Spring Boot Flo" category-id="information" effort="0">
                    <description>The application embeds Spring Boot Flo.</description>
                </classification>
                <technology-tag level="INFORMATIONAL">Spring Boot Flo</technology-tag>
            </perform>
            <where param="prefixes">
                <matches pattern="(&lt;|')"/>
             </where>
             <where param="extensions">
                 <matches pattern="(html|ts)"/>
             </where>
        </rule>
        <rule id="embedded-framework-08700">
            <when>
                <javaclass references="org.springframework.scheduling.annotation.Scheduled">
                    <location>ANNOTATION</location>
                </javaclass>
            </when>
            <perform>
                <classification title="Embedded framework - Spring Scheduled" category-id="information" effort="0">
                    <description>The application embeds Spring Scheduled.</description>
                </classification>
                <technology-tag level="INFORMATIONAL">Spring Scheduled</technology-tag>
            </perform>
        </rule>    
        <!-- this rule has to be always the last one in order to effectively checks for classifications added from previous rules in this ruleset-->
        <rule id="embedded-framework-09000">
            <when>
                <file filename="spring{*}.jar"/>
            </when>
            <perform>
                <iteration>
                    <when>
                        <not>
                            <has-classification title="Embedded framework - Spring {pattern}"/>
                        </not>
                    </when>
                    <classification title="Embedded framework - Spring" category-id="information" effort="0">
                        <description>The application embeds the Spring framework.</description>
                    </classification>
                    <technology-tag level="INFORMATIONAL">Spring</technology-tag>
                </iteration>
            </perform>
            <where param="pattern">
                <matches pattern="(Integration|Data|Security|MVC|Batch|Boot|Test|DI)"/>
            </where>
        </rule>
        <!-- the rule above has to be always the last one in order to effectively checks for classifications added from previous rules in this ruleset-->
    </rules>
