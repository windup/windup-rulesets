                </or>
            </when>
            <perform>
                    <hint effort="1" severity="mandatory">
                        <message>
                            This is a websphere specific interface and needs to be migrated to javax.jms.TopicConnectionFactory.
                        </message>
                        <link href="TODO" title="JMS migration"/>
                        <tag>JMS</tag>
